<!-- wp:heading {"level":3} -->
<h3>PRIMEIROS ANOS</h3>
<!-- /wp:heading -->

<!-- wp:paragraph -->
{{paragraphs[0]}}
<!-- /wp:paragraph -->

<!-- wp:heading {"level":3} -->
<h3>VIDA E OBRA</h3>
<!-- /wp:heading -->

<!-- wp:paragraph -->
{{paragraphs[1]}}
<!-- /wp:paragraph -->

<!-- wp:image {"align":"center","id":14004,"sizeSlug":"large","linkDestination":"none"} -->
<div class="wp-block-image">
    <figure class="aligncenter size-large"><img
            src="{{images[0]}}" alt=""
            class="wp-image-14004" />
        <figcaption>{{title}}. Imagem: Wikimedia Commons.</figcaption>
    </figure>
</div>
<!-- /wp:image -->

{% for paragraph in paragraphs[2:-2] %}

<!-- wp:paragraph -->
{{paragraph}}
<!-- /wp:paragraph -->

{% endfor %}

<!-- wp:image {"align":"center","id":14059,"width":768,"height":595,"sizeSlug":"large","linkDestination":"none"} -->
<div class="wp-block-image">
    <figure class="aligncenter size-large is-resized"><img
            src="{{images[1]}}" alt=""
            class="wp-image-14059" width="768" height="595" />
        <figcaption>{{title}}. Imagem: Wikimedia Commons.
        </figcaption>
    </figure>
</div>
<!-- /wp:image -->

<!-- wp:heading {"level":3} -->
<h3>MORTE E LEGADO</h3>
<!-- /wp:heading -->

<!-- wp:paragraph -->
{{paragraphs[-2]}}
<!-- /wp:paragraph -->

<!-- wp:pullquote {"className":"is-style-solid-color"} -->
<figure class="wp-block-pullquote is-style-solid-color">
    <blockquote>
        <p>{{quotes}}</p>
        <cite>{{title}}</cite>
    </blockquote>
</figure>
<!-- /wp:pullquote -->

<!-- wp:paragraph -->
{{paragraphs[-1]}}
<!-- /wp:paragraph -->

<!-- wp:paragraph -->
<p>REFERÊNCIAS:</p>
